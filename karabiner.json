              {
                "description": "Change caps_lock to left_control if pressed with other keys, change caps_lock to escape if pressed alone.",
                "from": {
                  "key_code": "caps_lock",
                  "modifiers": {
                    "optional": [
                      "any"
                    ]
                  }
                },
                "to": [
                  {
                    "key_code": "left_control"
                  }
                ],
                "to_if_alone": [
                  {
                    "key_code": "escape"
                  }
                ],
                "type": "basic"
              },
              {
                "description": "Control -> Hyper Key",
                "from": {
                  "key_code": "left_control",
                  "modifiers": {
                    "optional": [
                      "any"
                    ]
                  }
                },
                "to": [
                  {
                    "set_variable": {
                      "name": "hyper",
                      "value": 1
                    }
                  }
                ],
                "to_after_key_up": [
                  {
                    "set_variable": {
                      "name": "hyper",
                      "value": 0
                    }
                  }
                ],
                "type": "basic"
              }
            ]
